<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox id="vbox-with-background" alignment="CENTER" stylesheets="@../css/selectmode.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gomoku.project04gomoku.mvc.ViewModel.SelectModeController">
    <children>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
            <children>
                <Button fx:id="SinglePlayer" mnemonicParsing="false" onAction="#GoToSinglePlayer" text="Single Player (AI)" />
                <Pane prefHeight="20.0" prefWidth="100.0" />
                <Button fx:id="MultiplePlayer" mnemonicParsing="false" onAction="#GoToMultiplePlayer" text="Local Multiplayer" />
                <Pane prefHeight="20.0" prefWidth="100.0" />
                <Button fx:id="GoBackMain" mnemonicParsing="false" onAction="#GoBackToMain" text="Back" />
            </children>
        </VBox>
    </children>
</VBox>
